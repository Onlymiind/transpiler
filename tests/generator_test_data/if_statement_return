func foo() int {
    if false {
        return 1;
    } else {
        return 2;
    }
}

func foo2() int {
    if false {
        return 1;
    } else if 7 > 8{
        return 2;
    } else {
        return 3;
    }
}

func foo3() int {
    if false {
        return 1;
    } else if 3 == 4{
        if false {
            return 5;
        } else {
            return 4;
        }
    } else {
        return 3;
    }
}

func foo4() int {
    if false {
        return 1;
    } else if 5 <= 6{
        if false {
            
        } else {
            return 4;
        }
        return 6;
    } else {
        return 3;
    }
}

func main() {
    foo();
    foo2();
    foo3();
    foo4();
}
